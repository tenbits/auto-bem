{
  "name": "auto-bem",
  "description": "Transforms selectors in html and css to be unique",
  "version": "0.1.12",
  "author": "Alexander Kit <alex.kit@atmajs.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tenbits/autoBem.git"
  },
  "bugs": {
    "url": "https://github.com/tenbits/autoBem/issues"
  },
  "licenses": [
    "MIT"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@types/node": "^8.0.53",
    "app-bundler": "0.0.37",
    "atma": ">0.9.26",
    "atma-loader-ts": "^1.0.0",
    "dts-bundle": "^0.7.3"
  },
  "scripts": {
    "test": "atma test",
    "build": "app-bundler && npm run build-types",
    "watch": "app-bundler --watch",
    "release": "atma release",
    "build-types": "tsc -p . & atma custom tools/build-dts"
  },
  "keywords": [],
  "dependencies": {
    "maskjs": ">=0.61.5"
  },
  "app-bundler": {
    "file": "src/index.ts",
    "outputMain": "lib/index.js",
    "package": {
      "module": "commonjs",
      "moduleWrapper": "umd",
      "moduleName": "AutoBem",
      "commonjs": {
        "output": "simplified"
      }
    },
    "dynamicDependencies": [
      "maskjs"
    ],
    "defaultExtensions": {
      "js": "ts"
    },
    "middlewares": {
      "js": [],
      "ts": [
        "atma-loader-ts:read"
      ]
    }
  },
  "atma": {
    "plugins": [
      "atma-loader-ts"
    ],
    "settings": {
      "include": {
        "amd": true,
        "extentionDefault": {
          "js": "ts"
        }
      },
      "io": {
        "extensions": {}
      },
      "atma-loader-ts": {
        "extensions": [
          "ts",
          "test"
        ],
        "typescript": {
          "compilerOptions": {
            "module": "CommonJS",
            "#if TEST": {
              "module": "AMD"
            },
            "sourceMap": false
          }
        }
      }
    }
  },
  "tonicExampleFilename": "examples/simple.js"
}
