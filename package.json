{
  "name": "auto-bem",
  "description": "Transforms selectors in html and css to be unique",
  "version": "0.1.12",
  "author": "Alexander Kit <alex.kit@atmajs.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tenbits/autoBem.git"
  },
  "bugs": {
    "url": "https://github.com/tenbits/autoBem/issues"
  },
  "licenses": [
    "MIT"
  ],
  "main": "lib/autoBem.js",
  "devDependencies": {
    "app-bundler": "0.0.21",
    "atma": ">0.9.26",
    "atma-loader-babel": "^1.1.30",
    "atma-io-middleware-eslint": "latest",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-preset-es2015": "^6.24.1"
  },
  "scripts": {
    "test": "atma test",
    "build": "app-bundler",
    "watch": "app-bundler --watch",
    "release": "atma release"
  },
  "keywords": [],
  "dependencies": {
    "maskjs": ">=0.61.5"
  },
  "app-bundler": {
    "file": "src/exports.js",
    "outputMain": "lib/auto-bem.js",
    "package": {
      "module": "commonjs",
      "moduleWrapper": "umd",
      "moduleName": "AutoBemMask",
      "commonjs": {
        "output": "simplified"
      }      
    },
    "dynamicDependencies": [
      "maskjs"
    ],
    "middlewares": {
      "js": [
        "importer:read",
        "atma-io-middleware-eslint:read",
        "atma-loader-babel:read"
      ]
    }
  },
  "atma": {
    "plugins": [
      "atma-loader-babel",
      "atma-io-middleware-eslint"
    ],
    "settings": {
      "io": {
        "extensions": {
          "js": [
            "atma-io-middleware-eslint:read"
          ]
        }
      },
      "atma-loader-babel": {
        "extensions": [
          "es6",
          "test"
        ],
        "babel": {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "async-to-promises"
          ]
        }
      },
      "atma-io-middleware-eslint": {
        "parserOptions": {
          "ecmaVersion": 8
        }
      }
    }
  },
  "tonicExampleFilename": "examples/simple.js"
}
